<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SmsCodeEditText">
        <attr name="parting_line_color" format="color" />
        <attr name="point_color" format="color" />
    </declare-styleable>
    <declare-styleable name="RedImageView">
        <attr name="r_percent" format="float" />
        <attr name="shape" format="integer">
            <enum name="circular" value="1" />
            <enum name="square" value="2" />
        </attr>
        <attr name="pointColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="redPointVisibility" format="enum">
            <enum name="invisible" value="4" />
            <enum name="visible" value="0" />
        </attr>
        <attr name="withStroke" format="boolean" />
        <attr name="strokeColor" format="color" />
        <attr name="strokeWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="dashline">
        <attr name="lineColor" format="color" />
        <attr name="orientation">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ExpandTextView">
        <attr name="showLines" format="integer"/>
    </declare-styleable>

    <declare-styleable name="smile_rating">
        <!--填充图片-->
        <attr name="star_img" format="reference" />
        <!--默认图片-->
        <attr name="unstar_img" format="reference" />
        <!--图片宽度-->
        <attr name="image_width" format="dimension" />
        <!--图片高度-->
        <attr name="image_height" format="dimension" />
        <!--图片之间的间距-->
        <attr name="image_padding" format="dimension" />
        <!--图片总数-->
        <attr name="star_count" format="integer" />
        <!--填充的图片数量-->
        <attr name="star" format="integer" />
        <!--是否可以点击-->
        <attr name="clickable" format="boolean" />
    </declare-styleable>
</resources>